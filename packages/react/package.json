{
  "name": "@fekit/react",
  "author": "livemehere",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/index.js",
  "description": "Useful react components and hooks",
  "keywords": [
    "react",
    "react-hooks",
    "react-ui",
    "react-components"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./ui": {
      "import": {
        "default": "./dist/ui/index.mjs",
        "types": "./dist/ui/index.d.mts"
      },
      "require": {
        "default": "./dist/ui/index.js",
        "types": "./dist/ui/index.d.ts"
      }
    },
    "./hooks": {
      "import": {
        "default": "./dist/hooks/index.mjs",
        "types": "./dist/hooks/index.d.mts"
      },
      "require": {
        "default": "./dist/hooks/index.js",
        "types": "./dist/hooks/index.d.ts"
      }
    }
  },
  "dependencies": {
    "@fekit/core": "workspace:*"
  },
  "peerDependencies": {
    "@fekit/core": "*"
  }
}
