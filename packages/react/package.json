{
  "name": "@fekit/react",
  "author": "livemehere",
  "version": "0.0.1",
  "license": "MIT",
  "main": "dist/index.js",
  "description": "Useful react components and hooks",
  "keywords": [
    "react",
    "react-hooks",
    "react-ui",
    "react-components"
  ],
  "files": [
    "dist",
    "package.json"
  ],
  "exports": {
    "./hooks": {
      "import": {
        "default": "./dist/hooks/index.mjs",
        "types": "./dist/hooks/index.d.mts"
      },
      "require": {
        "default": "./dist/hooks/index.js",
        "types": "./dist/hooks/index.d.ts"
      }
    },
    ".": {
      "import": {
        "default": "./dist/index.mjs",
        "types": "./dist/index.d.mts"
      },
      "require": {
        "default": "./dist/index.js",
        "types": "./dist/index.d.ts"
      }
    }
  },
  "dependencies": {
    "@fekit/core": "workspace:*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "peerDependencies": {
    "@fekit/core": "*",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3"
  }
}
